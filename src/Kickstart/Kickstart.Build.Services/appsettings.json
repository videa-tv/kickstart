{
  "Port": 50095,
  "SeriLog": {
    "WriteTo": [
      {
        "Name": "Console",
        "theme": "AnsiConsoleTheme.Code",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss,fff} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ],
    "MinimumLevel": "Debug"
  },
  "ConnectionStrings": {
    "Build": "Server = localhost; Database =; Trusted_Connection = True; "
  },
  "Authentication": {
    "Enabled": false,
    "ApiKey": ""
  }
}